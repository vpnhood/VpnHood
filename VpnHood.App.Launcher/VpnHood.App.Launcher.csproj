<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>Exe</OutputType>
    <TargetFramework>net7.0</TargetFramework>
    <Authors>VpnHood</Authors>
    <Company>VpnHood</Company>
    <Copyright>2022 VpnHood</Copyright>
    <PackageLicenseExpression>LGPL-2.1-only</PackageLicenseExpression>
    <RepositoryUrl>https://github.com/vpnhood/vpnhood</RepositoryUrl>
    <PackageProjectUrl>https://github.com/vpnhood/vpnhood</PackageProjectUrl>
    <AssemblyName>run</AssemblyName>
    <PackageId>VpnHood.App.Launcher</PackageId>
    <PackageIcon>VpnHood.png</PackageIcon>
    <Product>VpnHood.App.Launcher</Product>
    <ApplicationIcon>Resources\VpnHood.ico</ApplicationIcon>
    <Version>2.4.325</Version>
    <AssemblyVersion>2.4.325</AssemblyVersion>
    <FileVersion>2.4.325</FileVersion>
    <Nullable>enable</Nullable>
    <LangVersion>latest</LangVersion>
  </PropertyGroup>

  <ItemGroup>
    <None Include="..\VpnHood.png" Link="Resources\VpnHood.png">
      <Pack>True</Pack>
      <PackagePath></PackagePath>
    </None>
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.Extensions.Logging.Abstractions" Version="7.0.0" />
  </ItemGroup>

</Project>
