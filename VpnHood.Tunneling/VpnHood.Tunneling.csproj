<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.1</TargetFramework>
    <RootNamespace>VpnHood.Tunneling</RootNamespace>
    <AssemblyName>VpnHood.Tunneling</AssemblyName>
    <Authors>VpnHood</Authors>
    <Company>VpnHood</Company>
    <PackageLicenseExpression>LGPL-2.1-only</PackageLicenseExpression>
    <PackageProjectUrl>https://github.com/vpnhood/vpnhood</PackageProjectUrl>
    <RepositoryUrl>https://github.com/vpnhood/vpnhood</RepositoryUrl>
    <PackageTags></PackageTags>
    <Copyright>2022 VpnHood</Copyright>
    <PackageId>VpnHood.Tunneling</PackageId>
    <PackageIcon>VpnHood.png</PackageIcon>
    <Product>VpnHood.Tunneling</Product>
    <Description>Provides tunnelling classes and protocols shared between VpnHood.Client and VpnHood.Server.</Description>
    <Version>2.6.330</Version>
    <AssemblyVersion>2.6.330</AssemblyVersion>
    <FileVersion>2.6.330</FileVersion>
    <Nullable>enable</Nullable>
    <LangVersion>latest</LangVersion>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.Extensions.Logging" Version="7.0.0" />
    <PackageReference Include="Microsoft.Extensions.Logging.Abstractions" Version="7.0.0" />
    <PackageReference Include="Microsoft.Extensions.Logging.Console" Version="7.0.0" />
    <PackageReference Include="PacketDotNet" Version="1.4.7" />
    <PackageReference Include="System.Text.Json" Version="7.0.1" />
  </ItemGroup>

  <ItemGroup>
    <Folder Include="Resources\" />
    <Folder Include="Properties\PublishProfiles\" />
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\VpnHood.Common\VpnHood.Common.csproj" />
  </ItemGroup>

  <ItemGroup>
    <None Include="..\VpnHood.png" Link="Resources\VpnHood.png">
      <Pack>True</Pack>
      <PackagePath></PackagePath>
    </None>
  </ItemGroup>

</Project>
